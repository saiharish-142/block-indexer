{
  "title": "Block Explorer Backend",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "Indexer Blocks Processed",
      "targets": [
        { "expr": "rate(indexer_blocks_processed_total[5m])" }
      ]
    },
    {
      "type": "graph",
      "title": "Indexing Lag Seconds",
      "targets": [
        { "expr": "indexer_lag_seconds" }
      ]
    },
    {
      "type": "graph",
      "title": "API Latency p95",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(api_request_latency_seconds_bucket[5m])) by (le))" }
      ]
    },
    {
      "type": "stat",
      "title": "Active WS Connections",
      "targets": [
        { "expr": "sum(ws_connections)" }
      ]
    }
  ],
  "schemaVersion": 37,
  "version": 1
}
